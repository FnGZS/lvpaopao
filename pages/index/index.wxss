.userInfo {
  width: 100%;
  height: 350rpx;
  background: url('https://clgl.ypc.edu.cn/vehicle_img/ypcar_broad.png? + Math.random() / 9999');
  background-size: 100% 150%;
  position: relative;
  box-shadow: 0rpx 0rpx 100rpx 0rpx #575757;
}

.userinfo-avatar {
  position: absolute;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  border: 5rpx solid white;
  left: 50%;
  margin-left: -64rpx;
  margin-top: 60rpx;
}

.userinfo {
  width: 50%;
  position: absolute;
  top: 200rpx;
  display: flex;
  justify-content: center;
  left: 25%;
  opacity: 0.85;
}

.userinfo-identity {
  background: #CBCBCB;
